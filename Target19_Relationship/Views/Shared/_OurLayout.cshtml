<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - マイ ASP.NET アプリケーション</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("(株)ふじ川工具店", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">日常業務<b class="create"></b></a>
                        <ul class="dropdown-menu multi-level">
                            <li class="dropdown-submenu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">受注</a>
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("Create","Order")">受注入力</a></li>
                                    <li><a href="@Url.Action("SetSellingPrice","Order")">価格設定</a></li>
                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">発注</a>
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("Statement","Purchase")">詳細入力</a></li>
                                    <li><a href="@Url.Action("Select","Purchase")">発信</a></li>
                                </ul>
                            </li>
                            <li><a href="@Url.Action("Accept", "Warehousing")">仕入</a></li>
                            <li class="dropdown-submenu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">会計</a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">入金</a></li>
                                </ul>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">月次処理<b class="create"></b></a>
                        <ul class="dropdown-menu multi-level">
                            <li class="dropdown-submenu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">値引き</a>
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("SalesDiscount", "Accounting")">売上値引き</a></li>
                                    <li><a href="@Url.Action("PurchaseDiscount", "Accounting")">仕入値引き</a></li>
                                </ul>
                            </li>
                            <li><a href="#">買掛請求書</a></li>
                            <li><a href="#">売掛請求書</a></li>
                            <li><a href="#">会計書類</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">決算処理<b class="create"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">期末残高</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">情報閲覧<b class="create"></b></a>
                        <ul class="dropdown-menu  multi-level">
                            <li class="dropdown-submenu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">マスターデータ</a>
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("AccountTitleList", "MasterData")">勘定科目</a></li>
                                    <li><a href="@Url.Action("BusinessPartnerList", "MasterData")">取引先</a></li>
                                    <li><a href="@Url.Action("ProductList", "MasterData")">商品</a></li>
                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">取引データ</a>
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("BeforeIssuingPurchaseOrderList", "TransactionData")">注文書発行前</a></li>
                                    <li><a href="@Url.Action("BeforeWarehousingList","TransactionData")">入庫前</a></li>
                                    <li><a href="@Url.Action("PurchaseList","TransactionData")">仕入</a></li>
                                    <li><a href="@Url.Action("History", "TransactionData")">納品書発行前</a></li>
                                    <li><a href="@Url.Action("SalesList", "TransactionData")">売上</a></li>
                                    <li><a href="@Url.Action("BeforeDelivery", "Sale")">納品前商品</a></li>
                                    <li><a href="@Url.Action("BeforePurchase", "Purchase")">入庫前商品</a></li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("問い合わせ", "Contact", "Home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - マイ ASP.NET アプリケーション</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datepicker")
    @RenderSection("scripts", required: false)
</body>
</html>
