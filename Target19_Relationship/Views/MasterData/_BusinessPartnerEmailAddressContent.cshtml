@model IEnumerable<Target19_Relationship.Models.Details.DetailBusinessPartnerEmailAddress>

<table class="table table-striped table-bordered"">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BusinessPartner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rank)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Note)
        </th>
        <th class="edit">修正</th><th class="edit">詳細</th><th class="edit">削除</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BusinessPartner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rank)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Note)
        </td>
        <td >
            <a href="~/MasterData/BusinessPartnerEmailAddressEdit/@item.Id"><span class="glyphicon glyphicon-pencil" 　aria-hidden="true"></span></a>
        </td>
        <td >
            <a href="~/MasterData/BusinessPartnerEmailAddressDetail/@item.Id"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span></a>
        </td>
        <td>
            <a href="~/MasterData/BusinessPartnerEmailAddressDelete/@item.Id"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></a>
        </td>
    </tr>
}

</table>
